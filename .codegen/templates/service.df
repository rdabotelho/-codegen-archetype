template {
	sourceFile: 'service.java'
	targetFile: 'src/main/java/${PACKAGE.convertDotToSlash}/service/${domain.name}Service.java'
	scope: 'entity'
	block(1, 4) {
	    replace('com.example.demo', PACKAGE)
		replace('DomainName', domain.name.pascalCase)
	}
	block(12, 37) {
		replace('DomainName', domain.name.pascalCase)
		replace('domainName', domain.name.lowerCase)
	}
}